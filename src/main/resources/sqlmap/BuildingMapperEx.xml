<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kgc.dao.BuildingMapperEx" >
  <resultMap id="BaseResultMap" type="com.kgc.pojo.Building" >
    <id column="id" property="id" jdbcType="INTEGER" />
    <result column="block" property="block" jdbcType="VARCHAR" />
    <result column="uint" property="uint" jdbcType="VARCHAR" />
    <result column="room_number" property="roomNumber" jdbcType="VARCHAR" />
    <result column="community_building" property="communityBuilding" jdbcType="INTEGER" />
  </resultMap>
  <insert id="insertBuilding" useGeneratedKeys="true" keyColumn="id" keyProperty="id" parameterType="Building">
    insert into beike_building (block, uint,
      room_number, community_building)
    values (#{block,jdbcType=VARCHAR}, #{uint,jdbcType=VARCHAR},
      #{roomNumber,jdbcType=VARCHAR}, #{communityBuilding,jdbcType=INTEGER})
  </insert>
</mapper>